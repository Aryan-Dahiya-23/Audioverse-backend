{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.js"
      },
      {
        "src": "/uploads/products/(.*)",
        "headers": {
          "Access-Control-Allow-Origin": "*"
        },
        "dest": "/uploads/products/$1"
      }
    ]
  }
  